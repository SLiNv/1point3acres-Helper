{
    "name": "1point3acres Helper",
    "version": "1.0",
    "description" : "1point3acres Helper",
    "manifest_version": 2,
    "permissions": ["tabs", "*://*.1point3acres.com/*", "declarativeContent", "storage"],
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "homepage_url": "https://github.com/SLiNv/1point3acres-Helper"
}